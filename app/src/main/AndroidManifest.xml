<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.homework.ksing.ksing">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ul"
        android:label="@string/app_name"
        android:roundIcon="@drawable/ul"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        tools:replace="android:icon,android:roundIcon,android:theme">
        <activity android:name=".activity.LoginActivity" />
        <activity android:name=".activity.KgLoginActivity" />
        <activity android:name=".ui.MainActivity" />
        <activity android:name=".ui.EvaluateActivity"  />
        <activity android:name=".activity.WelcomeActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activity.NotificationDemoActivity" />
        <activity android:name=".activity.SettingActivity" />
        <activity android:name=".ui.EditUserInfoActivity" />

        <service
            android:name=".Service.GetMsgService"
            android:enabled="true"
            android:exported="true" />

        <activity android:name=".ui.UpdateActivity"></activity>
    </application>

</manifest>